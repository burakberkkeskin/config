set nocompatible
filetype on
filetype plugin on
filetype indent on
syntax on
set number
@REM set cursorline
set shiftwidth=2
set tabstop=2
set expandtab
set nobackup
set scrolloff=10
set nowrap
set incsearch
set ignorecase
set smartcase
set showcmd
set showmode
set showmatch
set hlsearch
set history=1000